{  
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "chromeNotes.html",
      "default_title": "Notes for Chrome",
      "name": " "
   },
   "description": "Lets you store Notes and Memos in Chrome's Localstorage so you can access it offline too!",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   
   "manifest_version": 2,
   "name": "Note pad & Memo Pad for Chrome",
   "offline_enabled": true,
   "permissions": [ "tabs", "http://*/*", "https://*/*", "unlimitedStorage" ],
   "update_url": "http://clients2.google.com/service/update2/crx",

   "web_accessible_resources": [
   "chromeNotes.html",
   "icon.png",
   "icon48.png",
   "icon16.png",
   "icon128.png",
   "backimg.png",
   "jquery.js",
   "animations.js",
   "popup.js",
   "style.css"

  ],
  "version": "11.10"
}
